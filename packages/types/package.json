{
  "name": "@whatsapp-cloudapi/types",
  "version": "2.0.0",
  "type": "module",
  "exports": {
    "./webhook": {
      "types": "./dist/webhook/index.d.ts",
      "default": "./dist/webhook/index.js"
    },
